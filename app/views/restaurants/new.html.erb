<h1>Tell Us About Your New Restaurant!</h1>
<div class="container" style=margin 120px>
  <%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category %>
  <%= f.submit "Save!", class: "btn btn-success" %>
  <% end %>
</div>


